{
  "name": "bplab/compare-structure",
  "description": "This package compare data between database tables and their fields with models and transformers.",
  "license": "MIT",
  "keywords": [
    "compare structure",
    "model",
    "transformer",
    "database"
  ],
  "authors": [
    {
      "name": "Bojan PopoviÄ‡",
      "email": "bojanpopovic81@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "5.8.36"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "BpLab\\CompareStructure\\": "src",
      "BpLab\\Library\\": "src/library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BpLab\\CompareStructure\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
